    "@nuxtjs/i18n": "^8.0.0-rc.5",
    "@rollup/plugin-yaml": "^4.1.2",
    "@types/ua-parser-js": "^0.7.37",
    "defu": "^6.1.2",
    "js-yaml": "^4.1.0",
    "sass": "^1.69.0",
    "ua-parser-js": "^1.0.36",
    "vuetify": "^3.3.20"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.8.5",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.2",
    "@nuxt/schema": "^3.5.2",
    "@nuxt/test-utils": "^3.7.4",
    "@types/js-yaml": "^4.0.6",
    "@types/node": "^20",
    "eslint": "^8.51.0",
    "eslint-config-nuxt": "github:chika3742/eslint-config-nuxt",
    "nuxt": "^3.7.4",
    "vitest": "^0.34.6"
  }
