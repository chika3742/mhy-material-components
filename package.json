{
  "name": "mhy-material-components",
  "version": "0.0.0",
  "description": "Common components for genshin-material / hsr-material",
  "license": "MIT",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "yarn dev:prepare && nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "@nuxt/kit": "^3.5.2",
    "@nuxtjs/i18n": "^8.0.0-rc.3",
    "@rollup/plugin-yaml": "^4.1.1",
    "defu": "^6.1.2",
    "js-yaml": "^4.1.0",
    "sass": "^1.63.6",
    "vuetify": "^3.3.5"
  },
  "devDependencies": {
    "@nuxt/devtools": "^0.8.0",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.5.2",
    "@nuxt/test-utils": "^3.7.4",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18",
    "eslint": "^8.42.0",
    "nuxt": "^3.5.1",
    "vitest": "^0.34.6"
  }
}
