{
  "name": "mhy-material-components",
  "version": "0.0.0",
  "description": "Common components for genshin-material / hsr-material",
  "license": "MIT",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "yarn dev:prepare && nuxt-module-build build",
    "dev": "nuxi dev playground",
